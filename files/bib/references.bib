@article{khare2024EnhancingSmallObject,
	title = {Enhancing {Small} {Object} {Detection} through {Structure}-{Aware} and {Global} {Contextual} {Information}},
	volume = {24},
	issn = {1000-1239},
	url = {http://journalcrd.org/wp-content/uploads/9-CRD2314.pdf},
	doi = {https://doi.org/20.2040/crd.v24.2024.4259},
	abstract = {In the task of small object detection, limitations such as fewer pixels, lack of rich features, and difficulty in feature extraction often lead to issues like missed detections, false detections, and low accuracy in models. To address these issues, a small object detection algorithm based on multi-scale structure-aware and global context information is proposed. A multi-scale structure-aware module is designed for complex scenarios to better capture the detailed features of small objects, thereby enhancing the model's ability to detect objects of different sizes. To obtain more global features, a global context information module is designed using the advantages of Transformers in capturing long-range dependency features, effectively establishing connections between pixel points in different regions. To address the gradient explosion phenomenon during model training, a new weighted loss function, W-FIoU, is designed, significantly improving the convergence speed during training. Extensive experimental results show that the proposed method achieves better detection results compared to other classic lightweight methods. Compared to the benchmark model, the proposed model improves mAP50 and mAP50:95 by 6.4 and 4.6 percentage points respectively on the VisDrone dataset, and also performs well on the TinyPerson dataset.},
	language = {en},
	number = {1000},
	journal = {Computer Research and Development},
	author = {Khare, Deepak Kumar and Bhagat, Amit and Priya, R Vishnu and Nag, Prashant Kumar and Malviya, Sunil},
	month = aug,
	year = {2024},
	keywords = {MyPublication, In Press},
	pages = {85--99},
	file = {Khare et al. - 2024 - Enhancing Small Object Detection through Structure-Aware and Global Contextual Information.pdf:C\:\\Users\\prash\\My Drive (prashantnag.workmail@gmail.com)\\Digital Garden\\05 Resources\\ZotfilesPHD\\My Articles\\Khare et al. - 2024 - Enhancing Small Object Detection through Structure-Aware and Global Contextual Information.pdf:application/pdf},
